{
  "name": "limweijin-blog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm run clean && set TAILWIND_MODE=watch && next dev",
    "build": "next build && node ./scripts/generate-sitemap",
    "clean": "rm -rf ./.next",
    "serve": "next start",
    "analyze": "set ANALYZE=true && next build",
    "prepare": "husky install",
    "lint": "npx next lint",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "pscale:shadow": "pscale connect limweijin-blog shadow --port 3310",
    "pscale:main": "pscale connect limweijin-blog main --port 3309",
    "compose": "node ./scripts/compose.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@headlessui/react": "^2.2.8",
    "@mdx-js/mdx": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@notionhq/client": "^5.1.0",
    "@prisma/client": "^6.16.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.20",
    "canvas-confetti": "^1.6.0",
    "date-fns": "^4.1.0",
    "esbuild": "^0.25.9",
    "gray-matter": "^4.0.3",
    "image-size": "^1.0.2",
    "mdx-bundler": "^10.1.1",
    "next": "^15.5.3",
    "next-auth": "5.0.0-beta.16",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.8.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.1",
    "react": "^19.1.1",
    "react-datocms": "^7.2.2",
    "react-dom": "^19.1.1",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-prism-plus": "^2.0.1",
    "rehype-slug": "^6.0.0",
    "remark-code-titles": "^0.1.2",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^4.0.1",
    "remark-slug": "^7.0.1",
    "swr": "^2.3.6",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.3",
    "@svgr/webpack": "^8.1.0",
    "@types/canvas-confetti": "^1.4.2",
    "@types/github-slugger": "^1.3.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "dedent": "^1.7.0",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.3",
    "file-loader": "^6.0.0",
    "globby": "^14.1.0",
    "husky": "^9.1.7",
    "inquirer": "^12.9.5",
    "lint-staged": "^16.1.6",
    "next-compose-plugins": "^2.2.1",
    "next-remote-watch": "^2.0.0",
    "prettier": "^3.6.2",
    "prisma": "^6.16.1",
    "rehype": "^13.0.2",
    "remark-frontmatter": "^5.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "typescript": "^5.7.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
